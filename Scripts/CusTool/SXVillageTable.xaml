<controls:ProWindow x:Class="CCTool.Scripts.CusTool.SXVillageTable"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:controls="clr-namespace:ArcGIS.Desktop.Framework.Controls;assembly=ArcGIS.Desktop.Framework"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:extensions="clr-namespace:ArcGIS.Desktop.Extensions;assembly=ArcGIS.Desktop.Extensions"
        mc:Ignorable="d"
        Title="山西省村规结构调整表(亦求长生亦求你)" Height="550" Width="500"
        WindowStartupLocation="CenterScreen"
        Icon="/CCTool;component/Data/Icons/Atoolbox.png"
    >
    <controls:ProWindow.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/CCTool;component/Data/Themes/Generic.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </controls:ProWindow.Resources>
    <Grid>
        <Label Content="输入用地图层：" HorizontalAlignment="Left" Margin="18,19,0,0" VerticalAlignment="Top" Width="113" Height="25"/>
        <ComboBox x:Name="combox_fc" Style="{StaticResource ComboBox_icon}" Margin="131,21,16,0" VerticalAlignment="Top" DropDownOpened="combox_fc_DropDown" Height="21"/>
        <Label Content="选择输出Excel所在的文件夹：" HorizontalAlignment="Left" Margin="15,212,0,0" VerticalAlignment="Top" Width="375" Height="25"/>
        <TextBox x:Name="textFolderPath" Style="{StaticResource TextBoxNormal}" Margin="18,242,38,0" TextWrapping="NoWrap" Text="" VerticalAlignment="Top"/>
        <Button x:Name="openFolderButton" Style="{StaticResource btn_openFolder}" Margin="0,244,15,0" HorizontalAlignment="Right" VerticalAlignment="Top" Click="openFolderButton_Click"/>

        <Label Content="现状用地编码字段：" Margin="18,85,0,0" VerticalAlignment="Top" Height="26" HorizontalAlignment="Left" Width="113"/>
        <ComboBox x:Name="combox_bmField_xz" Style="{StaticResource ComboBox_icon}" Margin="131,87,16,0" VerticalAlignment="Top" DropDownOpened="combox_bmField_xz_DropDown" Height="22"/>
        <Button x:Name="btn_go" Style="{StaticResource btn_run}" Margin="0,0,16,28"  VerticalAlignment="Bottom" HorizontalAlignment="Right" Click="btn_go_Click" />

        <Button x:Name="btn_help" Style="{StaticResource btn_help}" Margin="0,0,98,28" VerticalAlignment="Bottom" HorizontalAlignment="Right" Click="btn_help_Click"/>
        <Label Content="规划用地编码字段：" Margin="18,118,0,0" VerticalAlignment="Top" Height="26" HorizontalAlignment="Left" Width="113"/>
        <ComboBox x:Name="combox_bmField_gh" Style="{StaticResource ComboBox_icon}" Margin="131,120,16,0" VerticalAlignment="Top" DropDownOpened="combox_bmField_gh_DropDown" Height="22"/>
        <Label Content="村庄名称字段：" Margin="18,51,0,0" VerticalAlignment="Top" Height="26" HorizontalAlignment="Left" Width="113"/>
        <ComboBox x:Name="combox_nameField" Style="{StaticResource ComboBox_icon}" Margin="131,53,16,0" VerticalAlignment="Top" DropDownOpened="combox_nameField_DropDown" Height="22"/>
        <Label Content="现状城镇村属性码：" Margin="18,152,0,0" VerticalAlignment="Top" Height="26" HorizontalAlignment="Left" Width="113"/>
        <ComboBox x:Name="combox_czcField_xz" Style="{StaticResource ComboBox_icon}" Margin="131,154,16,0" VerticalAlignment="Top" DropDownOpened="combox_czcField_xz_DropDown" Height="22"/>
        <Label Content="规划城镇村属性码：" Margin="18,185,0,0" VerticalAlignment="Top" Height="26" HorizontalAlignment="Left" Width="113"/>
        <ComboBox x:Name="combox_czcField_gh" Style="{StaticResource ComboBox_icon}" Margin="131,187,16,0" VerticalAlignment="Top" DropDownOpened="combox_czcField_gh_DropDown" Height="22"/>
        <GroupBox Header="面积参数" Margin="15,279,15,0" Height="90" VerticalAlignment="Top"/>
        <ComboBox x:Name="combox_unit" Margin="277,327,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.791,0.466" Height="25" HorizontalAlignment="Left" Width="180"  />
        <ComboBox x:Name="combox_area" Margin="27,327,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.791,0.466" Height="25" HorizontalAlignment="Left" Width="180" />
        <Label Content="面积类型：" HorizontalAlignment="Left" Margin="27,301,0,0" VerticalAlignment="Top" Width="125"/>
        <Label Content="面积单位：" HorizontalAlignment="Left" Margin="277,301,0,0" VerticalAlignment="Top" Width="125"/>


    </Grid>
</controls:ProWindow>
