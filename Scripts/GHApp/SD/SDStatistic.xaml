<controls:ProWindow x:Class="CCTool.Scripts.GHApp.SD.SDStatistic"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:controls="clr-namespace:ArcGIS.Desktop.Framework.Controls;assembly=ArcGIS.Desktop.Framework"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:extensions="clr-namespace:ArcGIS.Desktop.Extensions;assembly=ArcGIS.Desktop.Extensions"
        mc:Ignorable="d"
        Title="三调_土地利用现状面积汇总表" Height="524" Width="620"
        WindowStartupLocation="CenterScreen" 
        Icon="/CCTool;component/Data/Icons/Atoolbox.png"
    >
    <controls:ProWindow.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/CCTool;component/Data/Themes/Generic.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </controls:ProWindow.Resources>
    <Grid>

        <GroupBox Header="按面要素分区域统计(可选)" Margin="15,62,41,0" Height="65" VerticalAlignment="Top"/>
        <GroupBox Header="统计参数" Margin="15,179,0,0" Height="203" VerticalAlignment="Top" HorizontalAlignment="Left" Width="253"/>
        <ComboBox x:Name="combox_fc" Style="{StaticResource ComboBox_icon}" Margin="112,21,41,0" VerticalAlignment="Top" DropDownOpened="combox_fc_DropDown" Height="25"/>
        <TextBox x:Name="textExcelPath" Style="{StaticResource TextBoxNormal}" Margin="112,141,41,0" TextWrapping="NoWrap" VerticalAlignment="Top"/>
        <Button x:Name="saveExcelButton" Style="{StaticResource btn_openFolder}" Margin="0,141,15,0" HorizontalAlignment="Right" VerticalAlignment="Top" Click="saveExcelButton_Click"/>

        <Button x:Name="btn_go" Style="{StaticResource btn_run}" Margin="0,0,15,30"  VerticalAlignment="Bottom" HorizontalAlignment="Right" Click="btn_go_Click" />


        <ComboBox x:Name="combox_unit" Margin="112,247,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.791,0.466" Height="25" HorizontalAlignment="Left" Width="110"  />
        <ComboBox x:Name="combox_fc_area"  Style="{StaticResource ComboBox_icon}"  Margin="108,87,0,0" VerticalAlignment="Top" DropDownOpened="combox_fc_area_DropDown" Height="25" DropDownClosed="combox_fc_area_Closed" HorizontalAlignment="Left" Width="160" />
        <ComboBox x:Name="combox_field_area"  Style="{StaticResource ComboBox_icon}"  Margin="405,87,0,0" VerticalAlignment="Top" DropDownOpened="combox_field_area_DropDown" Height="25" HorizontalAlignment="Left" Width="160" />
        <ComboBox x:Name="combox_digit" Margin="112,282,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.791,0.466" Height="25" HorizontalAlignment="Left" Width="110" />
        <ComboBox x:Name="combox_area" Margin="112,211,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.791,0.466" Height="25" HorizontalAlignment="Left" Width="110" />
        <Label Content="选择三调图层：" HorizontalAlignment="Left" Margin="15,21,0,0" VerticalAlignment="Top" Width="97"/>
        <Label Content="分区域图层：" HorizontalAlignment="Left" Margin="27,87,0,0" VerticalAlignment="Top" Width="106"/>
        <Label Content="面积类型：" HorizontalAlignment="Left" Margin="38,211,0,0" VerticalAlignment="Top" Width="65"/>
        <Label Content="分区域名称字段：" HorizontalAlignment="Left" Margin="297,87,0,0" VerticalAlignment="Top" Width="106"/>
        <Label Content="面积单位：" HorizontalAlignment="Left" Margin="38,247,0,0" VerticalAlignment="Top" Width="65"/>
        <Label Content="小数位数：" HorizontalAlignment="Left" Margin="38,282,0,0" VerticalAlignment="Top" Width="75"/>
        <Label Content="输出Excel路径：" HorizontalAlignment="Left" Margin="15,139,0,0" VerticalAlignment="Top" Width="97"/>

        <Button x:Name="btn_help" Style="{StaticResource btn_help}" Margin="0,0,97,30" VerticalAlignment="Bottom" HorizontalAlignment="Right" Click="btn_help_Click"/>
        <CheckBox x:Name="checkBox_adj" Content="计算平差" HorizontalAlignment="Left" Margin="42,323,0,0" VerticalAlignment="Top" IsChecked="True"/>
        <CheckBox x:Name="checkBox_vg" Content="分村统计(根据ZLDWMC字段)" HorizontalAlignment="Left" Margin="42,352,0,0" VerticalAlignment="Top" IsChecked="True" Width="180"/>
        <GroupBox Header="输出sheet表格" Margin="310,179,41,0" Height="203" VerticalAlignment="Top"/>
        <CheckBox x:Name="cb_yd" Content="1、土地利用现状分类面积汇总表" HorizontalAlignment="Left" Margin="327,216,0,0" VerticalAlignment="Top" IsChecked="True"/>
        <CheckBox x:Name="cb_yd_hori" Content="2、土地利用现状分类面积汇总表(竖版)" HorizontalAlignment="Left" Margin="327,244,0,0" VerticalAlignment="Top" IsChecked="True"/>
        <CheckBox x:Name="cb_yd_1" Content="3、土地利用现状一级分类面积汇总表" HorizontalAlignment="Left" Margin="327,273,0,0" VerticalAlignment="Top" IsChecked="True"/>
        <CheckBox x:Name="cb_yd_3" Content="4、土地利用现状三大类面积汇总表" HorizontalAlignment="Left" Margin="327,301,0,0" VerticalAlignment="Top" IsChecked="True"/>


    </Grid>
</controls:ProWindow>
